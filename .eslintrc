{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "react/jsx-quotes": 1,
    "quotes": [2, "single"],
    "comma-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-multi-spaces": 0,
    "key-spacing": 0
  }
}
